{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json.",
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"customDuotone": false,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [],
			"text": true,
			"heading": true,
			"button": true
		},
		"dimensions": true,
		"layout": {
			"contentSize": "640px",
			"wideSize": "1200px"
		},
		"spacing": true,
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"lineHeight": false,
			"dropCap": false,
			"fluid": true,
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true,
			"fontSizes": [
				{ "slug": "xs", "size": "0.8rem", "name": "XS" },
				{ "slug": "base", "size": "1rem", "name": "Base" },
				{ "slug": "small", "size": "1.25rem", "name": "Small (H6)" },
				{ "slug": "medium", "size": "1.56rem", "name": "Medium (H5)" },
				{
					"slug": "large",
					"size": "1.95",
					"name": "Large (H4)"
				},
				{ "slug": "xl", "size": "2.44rem", "name": "XL (H3)" },
				{ "slug": "2xl", "size": "3.05rem", "name": "3XL (H2)" },
				{ "slug": "3xl", "size": "3.81rem", "name": "3XL (H1)" }
			]
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "0"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--base)",
			"lineHeight": "calc(2px + 2ex + 2px)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--3-xl)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2-xl)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xl)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		}
	}
}
